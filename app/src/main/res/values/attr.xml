<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="hyTitleText" format="string"/>
    <attr name="hyTitleTextColor" format="color"/>
    <attr name="hyTitleTextSize" format="dimension"/>

    <declare-styleable name="ValidCodeView">
        <attr name="hyTitleText"/>
        <attr name="hyTitleTextColor"/>
        <attr name="hyTitleTextSize"/>
    </declare-styleable>

    <declare-styleable name="PopupView">
        <attr name="popupview_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="wareCount" format="integer"/>
        <attr name="wareWidth" format="integer"/>
        <attr name="android:color"/>
        <attr name="mode">
            <enum name="circle" value="-1"/>
            <enum name="triangle" value="-2"/>
        </attr>
    </declare-styleable>


</resources>